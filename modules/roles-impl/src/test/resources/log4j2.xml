<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>
  <Appenders>
    <Console name="console" target="SYSTEM_ERR">
      <PatternLayout pattern="%-80m -- %C{1}:%L %p%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <root level="info">
      <appender-ref ref="console"/>
    </root>
    <!--<Logger name="" level="debug"/>-->
  </Loggers>
</Configuration>
